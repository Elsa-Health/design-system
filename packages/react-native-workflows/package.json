{
  "name": "@elsa-ui/react-native-workflows",
  "version": "0.1.0",
  "description": "Elsa.Health React Native UI Library",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "components/",
    "theme/",
    "visuals/",
    "README.md",
    "!**/__tests__/**",
    "!**/__mocks__/**",
    "!**/__stories__/**",
    "!.circleci",
    "!.husky",
    "!example"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "build-types": "tsc -p .",
    "build-babel": "babel ./src --out-dir ./ --source-maps --extensions .ts,.tsx --ignore \"src/**/*.test.tsx\",\"src/**/*.test.js\",\"tsconfig.json\",\"tsconfig.build.json\"",
    "build": "yarn build-types && yarn build-babel",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "clean": "rm -rf node_modules && yarn cache clean && rm -rf ./helpers ./screens ./screen-groups tsconfig.tsbuildinfo",
    "bootstrap": "yarn example && yarn && yarn pods",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "build-site": "yarn build-storybook"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/elsa-health/react-native-elsa-ui",
  "author": "iam-kevin <kevin@elsa.health> (https://github.com/iam-kevin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elsa-health/react-native-elsa-ui/issues"
  },
  "homepage": "https://github.com/elsa-health/react-native-elsa-ui#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.12.5",
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/slider": "^4.2.2",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@release-it/conventional-changelog": "^2.0.0",
    "@storybook/addon-actions": "6.3",
    "@storybook/addon-controls": "6.3",
    "@storybook/addon-essentials": "6.3",
    "@storybook/addon-links": "6.3",
    "@storybook/addon-ondevice-actions": "^6.0.1-beta.5",
    "@storybook/addon-ondevice-backgrounds": "^6.0.1-beta.5",
    "@storybook/addon-ondevice-controls": "^6.0.1-beta.5",
    "@storybook/addon-ondevice-notes": "^6.0.1-beta.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-react-native-web": "^0.0.18",
    "@storybook/addons": "^6.5.3",
    "@storybook/cli": "^6.5.3",
    "@storybook/react": "^6.3",
    "@storybook/react-native": "^6.0.1-beta.6",
    "@storybook/testing-library": "^0.0.9",
    "@storybook/theming": "^6.5.3",
    "@testing-library/react-native": "^9.1.0",
    "@types/react": "^17.0.31",
    "@types/react-native": "^0.66.0",
    "@types/styled-components": "^5.1.24",
    "@types/styled-components-react-native": "^5.1.3",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-native-web": "^0.17.7",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "pod-install": "^0.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^2.0.5",
    "react": "^17",
    "react-dom": "17.0.2",
    "react-native": "^0.67",
    "react-native-paper": "^4.12.0",
    "react-native-storybook-loader": "^2.0.5",
    "react-native-svg": "^12.3.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-web": "^0.17.7",
    "react-test-renderer": "17.0.2",
    "release-it": "^14.2.2",
    "styled-components": "^5.3.5",
    "typescript": "^4.4.0"
  },
  "peerDependencies": {
    "@react-navigation/native": "*",
    "@react-navigation/native-stack": "*",
    "react": "*",
    "react-native": "*",
    "react-native-paper": "*",
    "react-native-svg": "*",
    "styled-components": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "./lib",
    "targets": [
      "commonjs",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@elsa-ui/react-native": "^0.1.0",
    "@react-native-picker/picker": "^2.4.0",
    "elsa-health-data-fns": "^1.5.8",
    "immer": "^9.0.12",
    "react-i18next": "^11.16.9",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-sectioned-multi-select": "^0.9.1",
    "zustand": "^3.6.0"
  }
}
